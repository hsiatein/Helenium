    "StatsService": {
        "duration": 60
    },
    "WebuiService": {
        "port": "4080",
        "session_buffer": 32
    },
    "ChatService": {
        "timeout_secs":20,
        "api": [
            {
                "base_url": "https://api.x.ai/v1",
                "model": "grok-4-1-fast-non-reasoning",
                "api_key_env_var": "GROK_API_KEY"
            },
            {
                "base_url": "https://generativelanguage.googleapis.com/v1beta/openai/",
                "model": "gemini-2.5-flash",
                "api_key_env_var": "GEMINI_API_KEY"
            },
            {
                "base_url": "https://api.x.ai/v1",
                "model": "grok-4-1-fast-reasoning",
                "api_key_env_var": "GROK_API_KEY"
            }
        ],
        "heleny": {
            "api": 0,
            "preset_path": "assets/presets/heleny.txt",
            "persona_path": "assets/presets/persona.txt"
        },
        "planner": {
            "api": 2,
            "preset_path": "assets/presets/planner.txt"
        },
        "executor": {
            "api": 2,
            "preset_path": "assets/presets/executor.txt"
        }
    },
    "MemoryService": {
        "short_term_length": 20,
        "storage_dir": "./storage",
        "display_length": 20
    },
    "ToolkitService": {
        "tools_dir": "./assets/tools"
    },
    "ToolsService": {
        "comfyui_config": {
            "base_url_env_var": "COMFYUI_BASE_URL",
            "base_auth_env_var": "COMFYUI_BASE_AUTH",
            "api_key_env_var": "COMFYUI_API_KEY",
            "base_prompt_path": "./assets/tool-resource/obs赫蕾妮-通用.json"
        }
    },
    "TaskService": {
        "max_running_tasks": 10,
        "max_working_loop": 100
    },
    "ScheduleService": {
        "schedule_dir": "./storage",
        "offset": 28800
    },
    "McpService": {
        "mcp_servers": {
            "duckduckgo": {
                "command": "docker",
                "args": [
                    "run",
                    "-i",
                    "--rm",
                    "mcp/duckduckgo"
                ]
            },
            "arxiv-mcp-server": {
                "command": "docker",
                "args": [
                    "run",
                    "-i",
                    "--rm",
                    "-e",
                    "ARXIV_STORAGE_PATH",
                    "-v",
                    "./.temp/arxiv:/local-directory",
                    "mcp/arxiv-mcp-server"
                ],
                "env": {
                    "ARXIV_STORAGE_PATH": "/Users/local-test/papers"
                }
            },
            "mcp-code-interpreter": {
                "command": "docker",
                "args": [
                    "run",
                    "-i",
                    "--rm",
                    "-v",
                    "./.temp/mcp-code-interpreter:/local-directory",
                    "mcp/mcp-code-interpreter"
                ]
            },
            "wikipedia-mcp": {
                "command": "docker",
                "args": [
                    "run",
                    "-i",
                    "--rm",
                    "mcp/wikipedia-mcp"
                ]
            },
            "puppeteer": {
                "command": "docker",
                "args": [
                    "run",
                    "-i",
                    "--rm",
                    "-e",
                    "DOCKER_CONTAINER",
                    "mcp/puppeteer"
                ],
                "env": {
                    "DOCKER_CONTAINER": "true"
                }
            },
            "filesystem": {
                "command": "docker",
                "args": [
                    "run",
                    "-i",
                    "--rm",
                    "-v",
                    "./.temp/filesystem:/local-directory",
                    "mcp/filesystem",
                    "/local-directory"
                ]
            },
            "dockerhub": {
                "command": "docker",
                "args": [
                    "run",
                    "-i",
                    "--rm",
                    "-e",
                    "HUB_PAT_TOKEN",
                    "mcp/dockerhub",
                    "--transport=stdio",
                    "--username=heleny"
                ],
                "env": {
                    "HUB_PAT_TOKEN": "your_docker_pat_token"
                }
            },
            "semgrep": {
                "command": "docker",
                "args": [
                    "run",
                    "-i",
                    "--rm",
                    "semgrep/semgrep",
                    "semgrep",
                    "mcp",
                    "-t",
                    "stdio"
                ]
            },
            "markitdown": {
                "command": "docker",
                "args": [
                    "run",
                    "-i",
                    "--rm",
                    "-v",
                    "./.temp/markitdown:/local-directory",
                    "mcp/markitdown"
                ]
            },
            "mcp-server-chart": {
                "command": "npx",
                "args": [
                    "-y",
                    "@antv/mcp-server-chart"
                ]
            },
            "excel": {
                "command": "uvx",
                "args": [
                    "excel-mcp-server",
                    "stdio"
                ]
            },
            "opendia": {
                "command": "npx",
                "args": [
                    "opendia"
                ]
            },
            "ppt": {
                "command": "uvx",
                "args": [
                    "--from",
                    "office-powerpoint-mcp-server",
                    "ppt_mcp_server"
                ],
                "env": {}
            },
            "word-document-server": {
                "command": "uvx",
                "args": [
                    "--from",
                    "office-word-mcp-server",
                    "word_mcp_server"
                ]
            },
            "node-code-sandbox": {
                "command": "docker",
                "args": [
                    "run",
                    "-i",
                    "--rm",
                    "mcp/node-code-sandbox"
                ]
            },
            "rust-mcp-filesystem": {
                "command": "docker",
                "args": [
                    "run",
                    "-i",
                    "--rm",
                    "-e",
                    "ENABLE_ROOTS",
                    "-e",
                    "ALLOW_WRITE",
                    "-v",
                    "./.temp/rust-mcp-filesystem:/local-directory",
                    "mcp/rust-mcp-filesystem",
                    "/local-directory"
                ],
                "env": {
                    "ENABLE_ROOTS": "false",
                    "ALLOW_WRITE": "false"
                }
            },
            "minecraft-wiki": {
                "command": "docker",
                "args": [
                    "run",
                    "-i",
                    "--rm",
                    "mcp/minecraft-wiki"
                ]
            },
            "paper-search": {
                "command": "docker",
                "args": [
                    "run",
                    "-i",
                    "--rm",
                    "mcp/paper-search"
                ]
            },
            "SQLite": {
                "command": "docker",
                "args": [
                    "run",
                    "-i",
                    "--rm",
                    "-v",
                    "./.temp/sqlite:/local-directory",
                    "mcp/sqlite",
                    "--db-path",
                    "/mcp/db.sqlite"
                ]
            },
            "danbooru-tags": {
                "command": "npx",
                "args": [
                    "-y",
                    "@smithery/cli@latest",
                    "run",
                    "@gamzadongza/danbooru-tags-mcp"
                ]
            }
        }
    }
}