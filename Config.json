{
    "Kernel": {
        "user_name": "Mugi"
    },
    "ConfigService": {
        "save_after": 10.0
    },
    "FsService": {
        "exchange_dir": "./.exchange",
        "temp_dir": "./.temp"
    },
    "AuthService": {
        "pub_keys": [
            "-----BEGIN PUBLIC KEY-----\nMCowBQYDK2VwAyEAUEsqCIV2CVWAJ1ScPMhZUi+fYer0ivp6ziucUF2TfGY=\n-----END PUBLIC KEY-----",
            "-----BEGIN PUBLIC KEY-----\nMCowBQYDK2VwAyEAG17AlUB4Smih6Z9bk7/FL3IyTm9USZD2aMuAWXBLa64=\n-----END PUBLIC KEY-----",
            "-----BEGIN PUBLIC KEY-----\nMCowBQYDK2VwAyEAezkZozB1KXNmhUxch+lYqWnO3Bwrh1smaFogHa4PTr0=\n-----END PUBLIC KEY-----"
        ]
